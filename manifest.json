{
  "manifest_version": 2,

  "name": "Profile Launcher",
  "description": "Quick launch Media",
  "version": "1.0.0",
  "icons": { "128": "images/icons8-100.png" },
  "browser_action": {
    "default_icon": "images/icons8-19.png",
    "default_popup": "HTML_files/popup.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
